# Copy this file to .env and fill in your API keys
# .env is gitignored - never commit your keys

# LLM Provider API Keys (provide the one matching your AGENT_PROVIDER)
OPENAI_API_KEY=sk-xxx
# ANTHROPIC_API_KEY=sk-ant-xxx
# GEMINI_API_KEY=xxx
# GROQ_API_KEY=xxx
# CEREBRAS_API_KEY=xxx
# XAI_API_KEY=xxx
# OPENROUTER_API_KEY=xxx
# MISTRAL_API_KEY=xxx

# Agent Configuration (optional - these have defaults)
# AGENT_PROVIDER=openai
# AGENT_MODEL=gpt-4o
# AGENT_WORKSPACE=/path/to/workspace
# AGENT_DATA_DIR=~/.tinycrab
# AGENT_PORT=8080
